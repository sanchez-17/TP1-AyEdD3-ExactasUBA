/******************************************************************************

https://www.spoj.com/AED32323/problems/main/ 

*******************************************************************************/
#include <algorithm>
#include <cstdio>
#include <vector>
#include <queue>
#include <cstring> //para memset
using namespace std;
int N;
vector<vector<int>> intersecciones; //cada subfila y las col con las que interseca
int cantSubfilas; //no creo q sea necesario pero capaz para debuggear
int cantSubcol;
int subfilas[N][N]; //reconstruyo el tablero pero cada casillero tiene la fila a la que pertenece
//esto para poder saber las intersecciones con las columnas.
int hueco = -1;

int main()
{
    int c;
    for(int i = 0; i < c; i++){
        cin>>N;
        cantSubfilas = 0;
        cantSubcol = 0;
        
        //recorro para 
        for(int j = 0; j < N; j++){
            int casilla;
            for(int k = 0; k < N; k++){
                cin>>casilla;
                if(casilla == 1){
                    cantSubfilas++;
                    subfilas[j][k] = hueco;
                }else{
                    subfilas[j][k] = cantSubfilas;
                }
            }
            if(casilla != 1){
                cantSubfilas++;
            } //osea si la casilla N no es hueco, pq sino estaria contando 2 veces
        }
        intersecciones =  vector<vector<int>>(cantSubfilas);
        
        //ahora recorro para las subcolumnas
        //me gustaria recorrelo por columna..
        for(int j = 0; j < N; j++){//col
            for(int k = 0; k < N; k++){//fil
                int subfila = subfilas[k][j];
                if(subfila != hueco){
                    int subcol = cantSubcol + cantSubfilas;//quiero darles un id unico
                    intersecciones[subfila].push_back(subcol);
                    /*
                    vector<int> subcolInter;
                    intersecciones.push_back(subcolInter);
                    intersecciones[subcol].push_back(t);
                    
                    ojo q queremos list ady undirected. y que no sabemos cual es t todavia
                    */
                }else{
                    cantSubcol++;
                }
            }
            if(subfila[N-1][j] != hueco) cantSubcol++;
        }
    }
    return 0;
}




